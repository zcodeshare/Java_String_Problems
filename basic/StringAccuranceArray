public class StringAccuranceArray {
    public static void main(String[] args) {
        String str = "deekshaa";
        int[] a = new int[256];

        for (int ch : str.toCharArray()) {
            a[ch]++;
        }

        for (int i = 0; i < a.length; i++) {
            if (a[i] > 0) {
                System.out.println((char)i + " â†’ " + a[i]);
            }
        }
    }
}

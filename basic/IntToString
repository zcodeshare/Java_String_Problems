public class IntToStringWith {
    public static void main(String[] args) {
        int num = 12345;
        String str = String.valueOf(num);
        System.out.println("Converted string: " + str);
    }
}

public class IntToStringWithout {
    public static void main(String[] args) {
        int num = 12345;
        String str = "";

        if (num == 0) str = "0";

        while (num > 0) {
            int digit = num % 10;
            str = (char)(digit + '0') + str;
            num /= 10;
        }

        System.out.println("Converted string without valueOf(): " + str);
    }
}


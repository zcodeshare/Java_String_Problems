public class SymmetricString {
    public static void main(String[] args) {
        String str = "abba";
        boolean symmetric = true;

        int n = str.length();
        for (int i = 0; i < n / 2; i++) {
            if (str.charAt(i) != str.charAt(n - i - 1)) {
                symmetric = false;
                break;
            }
        }

        if (symmetric)
            System.out.println("String is symmetric");
        else
            System.out.println("String is NOT symmetric");
    }
}

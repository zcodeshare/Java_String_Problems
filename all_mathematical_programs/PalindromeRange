public class PalindromeRange {
    public static void main(String[] args) {
        for (int i = 100; i <= 200; i++) {
            int n = i, rev = 0;
            while (n > 0) {
                rev = rev * 10 + n % 10;
                n /= 10;
            }
            if (rev == i) System.out.print(i + " ");
        }
    }
}

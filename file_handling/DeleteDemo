import java.io.File;

public class DeleteDemo {
    public static void main(String[] args) {
        File f = new File("demo.txt");

        if (f.exists()) {
            if (f.delete()) {
                System.out.println("File deleted");
            } else {
                System.out.println("Delete failed");
            }
        } else {
            System.out.println("File not found");
        }
    }
}
